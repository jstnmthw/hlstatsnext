  "policies": [
    {
      "vhost": "hlstats",
      "name": "hlstats-events-policy",
      "pattern": "hlstats\\.events\\..*",
      "apply-to": "queues",
      "definition": {
        "max-length": 100000,
        "overflow": "reject-publish",
        "message-ttl": 3600000,
        "dead-letter-exchange": "hlstats.events.dlx"
      },
      "priority": 1
    },
    {
      "vhost": "hlstats",
      "name": "hlstats-dlq-policy",
      "pattern": "hlstats\\.events\\.dlq",
      "apply-to": "queues",
      "definition": {
        "max-length": 10000,
        "overflow": "drop-head",
        "message-ttl": 86400000
      },
      "priority": 1
    }
  ],
  "queues": [
    {
      "name": "hlstats.events.priority",
      "vhost": "hlstats",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "hlstats.events.dlx",
        "x-message-ttl": 3600000,
        "x-max-priority": 10,
        "x-max-length": 50000
