    {
      "name": "hlstats.events.dlq",
      "vhost": "hlstats",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-max-length": 10000
      }
    }
  ],
  "exchanges": [
