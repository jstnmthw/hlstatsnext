NODE_ENV=development

# Logging Configuration  
# Supported levels: error, warn, info, debug
LOG_LEVEL=info

# Ingress Server Configuration
# UDP port for receiving game server logs (default: 27500)
INGRESS_PORT=27500

# RabbitMQ Configuration
RABBITMQ_URL=amqp://hlstats:hlstats@localhost:5672/hlstats
RABBITMQ_USER=hlstats
RABBITMQ_PASSWORD=hlstats
RABBITMQ_VHOST=hlstats

# MaxMind License Key
MAXMIND_KEY=
